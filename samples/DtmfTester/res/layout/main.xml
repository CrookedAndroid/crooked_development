<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
<EditText android:id="@+id/editDtmf" 
          android:layout_width="fill_parent" android:layout_height="wrap_content"></EditText>
<Button  
	android:id="@+id/sendDtmf"
    android:layout_width="fill_parent" 
    android:layout_height="wrap_content" 
    android:text="@string/sendDtmfString"
    />
<Button  
	android:id="@+id/sendDtmfStringBroadcast"
    android:layout_width="fill_parent" 
    android:layout_height="wrap_content" 
    android:text="@string/sendDtmfStringBroadcast"
    />
<Button
	android:id="@+id/startDtmf"
    android:layout_width="fill_parent" 
    android:layout_height="wrap_content" 
    android:text="@string/startDtmf"
    />
<Button
	android:id="@+id/stopDtmf"
    android:layout_width="fill_parent" 
    android:layout_height="wrap_content" 
    android:text="@string/stopDtmf"
    />

</LinearLayout>
